{
    "name": "kejawenlab/semart-hris",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.7",
        "api-platform/api-pack": "^1.0",
        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "kejawenlab/petruk-username": "^2.0",
        "league/csv": "^9.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/dotenv": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/profiler-pack": "^1.0",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "vich/uploader-bundle": "^1.6"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "phpstan/phpstan": "^0.9.1",
        "phpunit/dbunit": "^3.0",
        "phpunit/php-invoker": "^1.1",
        "phpunit/phpunit": "^6.4",
        "symfony/flex": "^1.0",
        "symfony/phpunit-bridge": "^3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KejawenLab\\Application\\SemartHris\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\KejawenLab\\Application\\SemartHris\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01BVZD4XBVC9W2SQTK5NK2173Q",
            "allow-contrib": true
        }
    }
}
